<h1>Contact List</h1>
<div class="fixed-action-btn" style="top: 45px; right: 24px;">
  <a id='add-btn' class="modal-trigger" href="#contact-form"><i class="material-icons">add</i></a>
</div>

  <!-- Modal Structure -->
  <div id="contact-form" class="modal">
    <form>
      <div class="modal-content">
      <h3>Placeholder</h3>
        <input type="hidden" name="contact[id]" id='id'>
        <div class="input-field">
          <label for="first_name" class="validate">First Name</label>
          <input type="text" name="contact[first_name]" id='first_name' ><br/>
        </div>
        <div class="input-field">
            <label for="last_name" class="validate">Last Name</label>
            <input type="text" name="contact[last_name]" id='last_name'><br/>
        </div>
        <div class="input-field">
            <label for="email" class="validate">Email</label>
            <input type="text" name="contact[email]" id='email'><br/>
        </div>
        <div class="input-field">
            <label for="phone_num" data>Phone Number</label>
            <input type="text" name="contact[phone_num]" id='phone_num'><br/>
        </div>
      </div>
      <div class="modal-footer">
        <input type="submit">
        <a href="#!" class="modal-action modal-close waves-effect waves-green btn-flat">cancel</a>
      </div>
    </form>
  </div>

<div class='row'>
  <div class='col s12'>
    <ul id="contact-list" class="collapsible popout">
    </ul>
  </div>
</div>